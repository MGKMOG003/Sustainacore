<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Sustainacore.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:Sustainacore.Mobile.Views"
    Shell.FlyoutBehavior="Disabled">

    <TabBar Route="Login">
        <ShellContent Title="Login" ContentTemplate="{DataTemplate views:LoginPage}" />
    </TabBar>

    <TabBar Route="Dashboard">
        <ShellContent Title="Dashboard" ContentTemplate="{DataTemplate views:DashboardPage}" />
    </TabBar>
</Shell>
